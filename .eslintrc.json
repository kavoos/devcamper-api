{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "no-unused-vars": "off",
    "no-useless-escape": "warn",
    "implicit-arrow-linebreak": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "prefer-destructuring": ["error", {
      "array": false,
      "object": true
    }
  }
}
